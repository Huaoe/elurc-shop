{
  "name": "payload-test",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "precheck": "yarn lint && yarn tsc --noEmit",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "seed:admin": "cross-env NODE_OPTIONS=--no-deprecation tsx src/scripts/seed-admin.ts",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "yarn run test:int && yarn run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" yarn exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "test:a11y": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/accessibility-audit.ts",
    "audit:a11y": "yarn test:a11y"
  },
  "dependencies": {
    "@faceless-ui/modal": "^3.0.0",
    "@faceless-ui/scroll-info": "^2.0.0",
    "@hookform/resolvers": "^3.3.4",
    "@payloadcms/db-postgres": "3.70.0",
    "@payloadcms/next": "3.70.0",
    "@payloadcms/richtext-lexical": "3.70.0",
    "@payloadcms/ui": "3.70.0",
    "@prisma/adapter-pg": "^6.0.0",
    "@prisma/client": "^6.0.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-email/components": "^1.0.4",
    "@solana/spl-token": "^0.4.0",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.95.0",
    "@testing-library/dom": "^10.4.1",
    "@types/bcryptjs": "^3.0.0",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "graphql": "^16.8.1",
    "lucide-react": "^0.562.0",
    "next": "15.4.10",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "payload": "3.70.0",
    "pg": "^8.16.3",
    "qrcode.react": "^3.1.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-email": "^5.2.2",
    "react-hook-form": "^7.71.0",
    "resend": "^6.7.0",
    "sharp": "0.34.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vite": "^7.3.1",
    "zod": "^3.22.4",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "axe-core": "^4.10.2",
    "jest-axe": "^9.0.0",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.7",
    "jsdom": "26.1.0",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "prettier": "^3.4.2",
    "prisma": "^6.0.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "packageManager": "yarn@4.5.3"
}
